{
    "homepage": "http://csdsdasdcwcwadx.github.io/penny-store",
    "name": "penny-store",
    "description": "create-react-app & redux & typescript sample",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "cross-env ENV=local PORT=3638 node scripts/start.js",
        "demo": "cross-env ENV=local SERVER=rel PUBLIC_URL=./ node scripts/build.js",
        "build": "cross-env ENV=rel SERVER=rel PUBLIC_URL=./ node scripts/build.js",
        "prod": "cross-env ENV=prod SERVER=prod PUBLIC_URL=./ node scripts/build.js",
        "test": "node scripts/test.js",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "commit": "cz",
        "lint": "stylelint src/**/*.scss --syntax scss",
        "lint:fix": "stylelint src/**/*.scss --syntax scss --fix",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build"
    },
    "dependencies": {
        "@n8tb1t/use-scroll-position": "^2.0.3",
        "@reduxjs/toolkit": "^1.2.5",
        "@types/react-responsive": "^8.0.5",
        "@types/react-scroll": "^1.8.5",
        "@types/sanitize-html": "^2.3.1",
        "@types/styled-components": "^5.1.7",
        "axios": "^0.21.1",
        "body-parser": "^1.19.0",
        "classnames": "^2.2.6",
        "cross-env": "^7.0.3",
        "dayjs": "^1.10.3",
        "firebase": "^9.17.2",
        "html-react-parser": "^1.2.4",
        "i18next": "^21.1.1",
        "normalize.css": "^8.0.1",
        "pubsub-js": "^1.9.4",
        "react": "^18.1.0",
        "react-app-polyfill": "^1.0.6",
        "react-dom": "^18.1.0",
        "react-i18next": "^12.0.0",
        "react-redux": "^8.0.5",
        "react-responsive": "^8.2.0",
        "react-scroll": "^1.8.8",
        "react-select": "^5.7.0",
        "react-slick": "^0.29.0",
        "redux": "^4.0.5",
        "redux-devtools-extension": "^2.13.8",
        "redux-saga": "^1.1.3",
        "sanitize-html": "^2.4.0",
        "slick-carousel": "^1.8.1",
        "styled-components": "^5.3.6"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.ts"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jest-environment-jsdom-fourteen",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.13.0",
        "@reduxjs/toolkit": "^1.2.5",
        "@storybook/addon-actions": "^6.1.11",
        "@storybook/addon-essentials": "^6.1.11",
        "@storybook/addon-links": "^6.1.11",
        "@storybook/react": "^6.1.11",
        "@svgr/webpack": "4.3.3",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "@types/classnames": "^2.2.11",
        "@types/jest": "^24.0.0",
        "@types/jquery": "^3.5.5",
        "@types/node": "^12.0.0",
        "@types/pubsub-js": "^1.8.3",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.9",
        "@types/react-redux": "^7.1.24",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "babel-eslint": "10.1.0",
        "babel-jest": "^24.9.0",
        "babel-loader": "8.1.0",
        "babel-plugin-named-asset-import": "^0.3.6",
        "babel-plugin-styled-components": "^1.12.0",
        "babel-preset-react-app": "^9.1.2",
        "camelcase": "^5.3.1",
        "case-sensitive-paths-webpack-plugin": "2.3.0",
        "css-loader": "3.4.2",
        "cz-conventional-changelog": "^3.3.0",
        "dotenv": "8.2.0",
        "dotenv-expand": "5.1.0",
        "eslint": "^7.32.0",
        "eslint-config-alloy": "^3.8.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-loader": "3.0.3",
        "eslint-plugin-flowtype": "4.6.0",
        "eslint-plugin-import": "2.20.1",
        "eslint-plugin-jsx-a11y": "6.2.3",
        "eslint-plugin-react": "^7.21.4",
        "eslint-plugin-react-hooks": "^1.7.0",
        "file-loader": "4.3.0",
        "fs-extra": "^8.1.0",
        "gh-pages": "^5.0.0",
        "html-webpack-plugin": "4.0.0-beta.11",
        "http-proxy-middleware": "^1.0.6",
        "identity-obj-proxy": "3.0.0",
        "jest": "24.9.0",
        "jest-environment-jsdom-fourteen": "1.0.1",
        "jest-resolve": "24.9.0",
        "jest-watch-typeahead": "0.4.2",
